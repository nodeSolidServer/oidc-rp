{
  "name": "oidc-rp-adapted",
  "version": "0.26.0",
  "description": "OpenID Connect Relying Party client library",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "npm run build:lib",
    "dist": "webpack --progress",
    "prepare": "npm run dist && npm run test",
    "preversion": "npm test",
    "test": "mocha --timeout=10000",
    "build:lib": "rm -rf lib && babel src --env-name production -d lib",
    "prebuildpackage": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedropiloto/oidc-rp-adapted.git"
  },
  "author": "Pedro Piloto",
  "license": "MIT",
  "homepage": "https://github.com/pedropiloto/oidc-rp-adapted",
  "dependencies": {
    "@solid/jose": "^0.5.0",
    "base64url": "^3.0.1",
    "isomorphic-webcrypto": "^2.3.2",
    "node-fetch": "^2.6.0",
    "standard-http-error": "^2.0.1",
    "whatwg-url": "^7.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "mocha": "^6.2.2",
    "nock": "^11.7.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
